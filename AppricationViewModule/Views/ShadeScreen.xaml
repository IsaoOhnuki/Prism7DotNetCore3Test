<UserControl x:Class="AppricationViewModule.Views.ShadeScreen"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:prism="http://prismlibrary.com/"
             xmlns:av="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="av"
             prism:ViewModelLocator.AutoWireViewModel="True"
             av:DesignWidth="211" av:DesignHeight="176">
    <UserControl.Resources>
        <ResourceDictionary Source="/CustomControlLibrary;component/ControlResource.xaml"/>
    </UserControl.Resources>
    <Border Background="{StaticResource ShadeBrush}"/>
</UserControl>
