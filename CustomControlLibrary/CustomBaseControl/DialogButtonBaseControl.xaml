<UserControl x:Class="CustomControlLibrary.CustomBaseControl.DialogButtonBaseControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:cb="clr-namespace:CustomControlLibrary.CustomBaseControl"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="250">
    <Button x:Name="button" Command="{Binding Command}" CommandParameter="{Binding CommandParameter}">
        <cb:LabelBaseControl Content="{Binding Content}"/>
    </Button>
</UserControl>
