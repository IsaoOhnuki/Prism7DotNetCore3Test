<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <AssemblyName>BlankCoreApp1</AssemblyName>
    <StartupObject>BlankCoreApp1.App</StartupObject>
    <EnableNETAnalyzers>true</EnableNETAnalyzers>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Prism.Unity" Version="8.0.0.1909" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\ApplicationLogicServiceLibrary\ApplicationLogicServiceModule.csproj" />
    <ProjectReference Include="..\AppricationViewModule\AppricationViewModule.csproj" />
    <ProjectReference Include="..\LogServiceLibrary\LogServiceLibrary.csproj" />
    <ProjectReference Include="..\ModelLibrary\ModelLibrary.csproj" />
    <ProjectReference Include="..\MvvmLibrary\MvvmServiceLibrary.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="Views\MessageDialogPage.xaml.cs">
      <SubType>Code</SubType>
    </Compile>
  </ItemGroup>
  <ItemGroup>
    <Page Update="Views\MessageDialogPage.xaml">
      <XamlRuntime>$(DefaultXamlRuntime)</XamlRuntime>
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Models\" />
    <Folder Include="Properties\" />
  </ItemGroup>
</Project>