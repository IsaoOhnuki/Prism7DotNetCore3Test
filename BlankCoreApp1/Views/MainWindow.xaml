<Window x:Class="BlankCoreApp1.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        xmlns:av="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:crm="clr-namespace:MvvmUtilityLibrary.ContentRegionMediator;assembly=MvvmUtilityLibrary"
        mc:Ignorable="av"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title}"
        av:DesignWidth="500"
        av:DesignHeight="300">
    <Grid>
        <ContentControl prism:RegionManager.RegionName="{Binding ContentRegionName}"
                        crm:ContentRegionMediator.MainContentRegion="True"/>
        <ContentControl prism:RegionManager.RegionName="{Binding OverwrapRegionName}"
                        crm:ContentRegionMediator.OverwrapContentRegion="True"/>
    </Grid>
</Window>
